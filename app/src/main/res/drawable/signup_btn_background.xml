<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
<item android:state_pressed="true">
    <shape>
        <stroke
            android:width="1dp"
            android:color="@android:color/transparent" />

        <solid android:color="@color/signup_btn_pressed_back_color" />

        <padding
            android:left="0dp"
            android:right="0dp"
            android:top="0dp"
            android:bottom="0dp" />

        <corners android:radius="@dimen/_23sdp" />
    </shape>
</item>

<item android:state_pressed="false">
    <shape>
        <stroke
            android:width="1dp"
            android:color="@android:color/transparent" />

        <solid android:color="@color/signup_btn_back_color" />

        <padding
            android:left="0dp"
            android:right="0dp"
            android:top="0dp"
            android:bottom="0dp" />

        <corners android:radius="@dimen/_23sdp" />
    </shape>
</item>
</selector>